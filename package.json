{
  "name": "invest-openapi-sandbox",
  "version": "1.0.0",
  "description": "Tinkoff.Invest sandbox server",
  "author": "Anton Skshidlevsky <meefik@gmai.com>",
  "main": "server.js",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "nconf": "^0.11.2",
    "ws": "^7.4.3"
  },
  "devDependencies": {
    "eslint": "^7.24.0",
    "eslint-config-standard": "^16.0.2"
  },
  "engines": {
    "node": ">= 12.0.0",
    "npm": ">= 6.0.0"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "extends": "standard",
    "rules": {
      "indent": [
        "error",
        2
      ],
      "semi": [
        "error",
        "always"
      ],
      "quotes": [
        "error",
        "single"
      ],
      "space-before-function-paren": [
        "error",
        "never"
      ]
    }
  }
}